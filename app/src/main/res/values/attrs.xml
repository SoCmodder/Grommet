<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AddressView">
        <attr name="address_type" format="enum">
            <enum name="MAILING" value="1"/>
            <enum name="PREVIOUS" value="2"/>
            <enum name="REGISTRATION" value="3"/>
            <enum name="ASSISTANT" value="4"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="NameView">
        <attr name="name_type" format="enum">
            <enum name="CURRENT" value="1"/>
            <enum name="PREVIOUS" value="2"/>
            <enum name="ASSISTANT" value="3"/>
        </attr>
    </declare-styleable>

</resources>
